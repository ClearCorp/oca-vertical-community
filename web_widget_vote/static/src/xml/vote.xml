<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

<t t-name="VoteBase">
    <div id="VoteBase">
    </div>
</t>


<t t-name="VoteFiveStar">
    <t t-foreach="widget.votes" t-as="vote">
        <p><t t-esc="vote.name"/><br/>
        <span class="oe_form_field oe_form_field_vote_five_star" t-att-name="vote.id" t-att-value="vote.value">
            <a t-att-name="vote.id" t-att-value="-2" class="oe_e star_vote_five_star oe_star_off">7</a>
            <a t-att-name="vote.id" t-att-value="-1" class="oe_e star_vote_five_star oe_star_off">7</a>
            <a t-att-name="vote.id" t-att-value="0" class="oe_e star_vote_five_star oe_star_off">7</a>
            <a t-att-name="vote.id" t-att-value="1" class="oe_e star_vote_five_star oe_star_off">7</a>
            <a t-att-name="vote.id" t-att-value="2" class="oe_e star_vote_five_star oe_star_off">7</a>
            <a t-att-name="vote.id" class="oe_e ok_vote_five_star" style="display:none;">8</a>
        </span>
        </p>
    </t>
<p>
<a id="VoteFiveStarClear" style="display:none;">Clear</a>
</p>

</t>
</templates>
