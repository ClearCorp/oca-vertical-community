<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="partner_robert" model="res.partner">
            <field name="name">Robert moderator</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="email">robert@yourcompany.example.com</field>
            <field name="street">12 petite rue</field>
            <field name="city">Paris</field>
            <field name="zip">75011</field>
            <field name="country_id" ref="base.fr"/>
        </record>

        <record id="user_robert" model="res.users">
            <field name="partner_id" ref="community.partner_robert"/>
            <field name="login">robert</field>
            <field name="password">robert</field>
            <field name="signature">--
Robert</field>
            <field name="presentation">Lorem ipsum</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('base.group_user'), ref('community.group_community_administrator')])]"/>
            <field name="moderator" eval="True"/>
            <field name="committee" eval="True"/>
        </record>


